# Define names for default workspaces for which we configure key bindings later on.
# Use variables to avoid repeating the names in multiple places.
set $ws01 "1"
set $ws02 "2"
set $ws03 "3"
set $ws04 "4"
set $ws05 "5"
set $ws06 "6"
set $ws07 "7"
set $ws08 "8"
set $ws09 "9"
set $ws10 "10"

workspace $ws01 output $display 
workspace $ws02 output $display 
workspace $ws03 output $display 
workspace $ws04 output $display 
workspace $ws05 output $display 
workspace $ws06 output $display
workspace $ws07 output $display 
workspace $ws08 output $display 
workspace $ws09 output $display 
workspace $ws10 output $display 
