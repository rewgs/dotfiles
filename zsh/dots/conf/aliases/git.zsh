# git commands
alias gap=git_add_all_and_push_with_commit_message_arg
alias gp="git pull"
alias gfarp="git fetch --all && git reset --hard && git pull"
alias grm=git_commit_readme
alias gdb="git_delete_branch $1 $2"
alias grb="git branch -r" # lists all remote branches
alias grbv="git branch -r -v" # lists all remote branches with the latest commit hash and commit message

# git clients
alias g="git-graph"
alias gg="git-igitt"
