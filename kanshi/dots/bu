profile laptop {
    output eDP-1 enable {
        mode 2880x1800@90Hz
        position 0,0 
        scale 2
    }
}


profile docked {
    output eDP-1 disable
    output DP-1 enable {
        mode 5120x2880@60Hz
        position 0,0
        scale 2
    }
    output DP-2 enable {
        mode 2560x2880@59.99900Hz
        position -2560,0
        scale 2
    }
}
