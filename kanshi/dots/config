profile laptop {
    output eDP-1 enable {
        mode 2880x1800@90Hz
        position 0,0 
        scale 2
    }
}


profile docked1 {
    output eDP-1 disable
    output DP-1 {
        mode 3840x2160@60Hz
        position 0,0 
        scale 2
    }
}


profile docked2 {
    output eDP-1 disable
    output DP-3 {
        mode 3840x2160@60Hz
        position 0,0 
        scale 2
    }
}
